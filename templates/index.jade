html
    meta(http-equiv="Content-Type" content="text/html; charset=utf-8")
    head
        文件索引
    body
        for classify in classifys
            p #{classify.name}
            ul 
                for book in classify.books
                    li: a(href="#{book.htmlUrlPath}") #{book.name}
